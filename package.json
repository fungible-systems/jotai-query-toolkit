{
  "name": "jotai-query-toolkit",
  "private": false,
  "version": "0.0.2",
  "description": "A toolkit for opinionated use of the react-query + Jotai integration.",
  "main": "index.js",
  "module": "esm/index.js",
  "types": "index.d.ts",
  "typesVersions": {
    "<4.0": {
      "esm/*": [
        "ts3.4/*"
      ],
      "*": [
        "ts3.4/*"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./index.d.ts",
      "module": "./esm/index.js",
      "default": "./index.js"
    }
  },
  "files": [
    "**"
  ],
  "sideEffects": false,
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "concurrently 'yarn:build:*'",
    "build:base": "rollup -c",
    "dev:build": "yarn build && yalc publish dist --push",
    "postbuild": "yarn copy",
    "eslint": "eslint --fix '{src,tests}/**/*.{js,ts,jsx,tsx}'",
    "eslint:ci": "eslint '{src,tests}/**/*.{js,ts,jsx,tsx}'",
    "pretest": "tsc --noEmit",
    "test": "NODE_ENV=test jest --passWithNoTests",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "typecheck": "tsc --noEmit",
    "prerelease": "yarn build",
    "release": "cd dist && npm publish",
    "copy": "downlevel-dts dist dist/ts3.4 && shx cp package.json dist && json -I -f dist/package.json -e \"this.private=false; this.devDependencies=undefined; this.optionalDependencies=undefined; this.scripts=undefined; this.husky=undefined; this.prettier=undefined; this.jest=undefined; this['lint-staged']=undefined;\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=12"
  },
  "prettier": "@stacks/prettier-config",
  "lint-staged": {
    "*.{js,ts,tsx,md}": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fungible-systems/jotai-query-utils.git"
  },
  "keywords": [
    "stacks",
    "web3",
    "small-bundle"
  ],
  "author": "Thomas Osmonson",
  "contributors": [],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fungible-systems/jotai-query-utils/issues"
  },
  "homepage": "https://github.com/fungible-systems/jotai-query-utils",
  "devDependencies": {
    "@babel/core": "7.14.6",
    "@babel/plugin-proposal-class-properties": "7.14.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.14.5",
    "@babel/plugin-transform-react-jsx": "7.14.5",
    "@babel/plugin-transform-runtime": "7.14.5",
    "@babel/plugin-transform-typescript": "7.14.6",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-typescript": "7.14.5",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-commonjs": "19.0.0",
    "@rollup/plugin-node-resolve": "13.0.0",
    "@rollup/plugin-typescript": "8.2.1",
    "@stacks/eslint-config": "1.0.9",
    "@stacks/prettier-config": "0.0.8",
    "@testing-library/react": "12.0.0",
    "@types/react": "17.0.11",
    "@types/react-dom": "17.0.8",
    "@typescript-eslint/eslint-plugin": "4.28.0",
    "@typescript-eslint/parser": "4.28.0",
    "acorn-jsx": "5.3.1",
    "babel-jest": "27.0.5",
    "concurrently": "6.2.0",
    "downlevel-dts": "0.7.0",
    "esbuild": "0.12.9",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "1.1.2",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jest": "24.3.6",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "fast-async": "6.3.8",
    "husky": "6.0.0",
    "jest": "27.0.5",
    "jotai": "^1.1.0",
    "json": "11.0.0",
    "lint-staged": "11.0.0",
    "next": "^11.0.1",
    "prettier": "2.3.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-query": "^3.17.2",
    "rollup": "2.52.3",
    "rollup-plugin-esbuild": "4.5.0",
    "rollup-plugin-size-snapshot": "0.12.0",
    "shx": "0.3.3",
    "ts-jest": "27.0.3",
    "tslib": "2.3.0",
    "typescript": "4.3.4"
  },
  "dependencies": {
    "fast-deep-equal": "3.1.3"
  },
  "peerDependencies": {
    "jotai": "1.1.0",
    "react": ">=16.8",
    "react-query": "*"
  }
}
